varchar Apellidos,
date fechaNacimiento,
varchar DNI,
varchar Direccion,
varchar Telefono,
);

Turno(
PK varchar codTurno,
varchar Descripcion,
);

Periodo(
PK date fechaInicio,
PK date fechaFin,
);

Equipo(
PK varchar codEquipo,
varchar Nombre,
);

PeticionServicio(
PK varchar codPetServicio,
varchar tipoServicio,
