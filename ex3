Sucursal(
PK int numSucursal,
varchar Ciudad,
Boolean Activo,
);

Cliente(
PK varchar DNI,
varchar Nombre,
varchar Apellidos,
varchar Direccion,
varchar Ciudad,
);

Cuenta(
PK int numCuenta,
double Saldo,
);

Transaccion(
PK double numTransaccion,
FK int numCuenta,
date Fecha,
varchar tipoOperacion,
double Cantidad,
);

Abrir(
PK interger abrir,
FK int numCuenta,
FK varchar DNI,
FK int numSucursal,
);

